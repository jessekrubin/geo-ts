{
  "name": "@jsse/geotypes",
  "type": "module",
  "version": "0.0.37",
  "description": "geo types",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./geojson": {
      "types": "./dist/esm/geojson.d.ts",
      "import": "./dist/esm/geojson.js",
      "require": "./dist/cjs/geojson.js"
    },
    "./lib/*": {
      "types": "./dist/esm/*",
      "import": "./dist/esm/*",
      "require": "./dist/cjs/*"
    },
    "./dist/*": {
      "types": "./dist/esm/*",
      "import": "./dist/esm/*",
      "require": "./dist/cjs/*"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build-fast": "tsdown src/geojson.ts --format cjs,esm",
    "typecheck": "tsc --noEmit -p tsconfig.json && tsc --noEmit -p tsconfig.tsd.json",
    "build": "pnpm run typecheck && pnpm run build:tsdown && pnpm run build:tsc",
    "build:tsc": "tsc -p tsconfig.json && tsc -p tsconfig.cjs.json",
    "build:indexes": "tsx ./build.mts",
    "build:tsdown": "tsdown --format cjs,esm --dts",
    "test": "vitest run",
    "dev": "vitest watch --coverage",
    "lint": "eslint .",
    "fmt": "prettier --write .",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@jsse/eslint-config": "^0.4.26",
    "@types/geojson": "^7946.0.16",
    "@types/node": "^25.0.6",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@vitest/coverage-v8": "^4.0.17",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "prettier": "3.7.4",
    "publint": "^0.3.16",
    "tsdown": "^0.19.0",
    "tsx": "^4.21.0",
    "typescript": "5.9.3",
    "vitest": "4.0.17",
    "zx": "^8.8.5"
  }
}
