{
  "name": "@jsse/geotypes",
  "type": "module",
  "version": "0.0.35",
  "description": "geo types",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./geojson": {
      "types": "./dist/esm/geojson.d.ts",
      "import": "./dist/esm/geojson.js",
      "require": "./dist/cjs/geojson.js"
    },
    "./lib/*": {
      "types": "./dist/esm/*",
      "import": "./dist/esm/*",
      "require": "./dist/cjs/*"
    },
    "./dist/*": {
      "types": "./dist/esm/*",
      "import": "./dist/esm/*",
      "require": "./dist/cjs/*"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build-fast": "tsup src/geojson.ts --format cjs,esm",
    "typecheck": "tsc --noEmit -p tsconfig.json && tsc --noEmit -p tsconfig.tsd.json",
    "build": "pnpm run typecheck && pnpm run build:tsup && pnpm run build:tsc",
    "build:tsc": "tsc -p tsconfig.json && tsc -p tsconfig.cjs.json",
    "build:indexes": "tsx ./build.mts",
    "build:tsup": "tsup --format cjs,esm --dts",
    "test": "vitest run",
    "dev": "vitest watch --coverage",
    "lint": "eslint .",
    "fmt": "prettier --write .",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@jsse/eslint-config": "^0.2.30",
    "@types/geojson": "^7946.0.15",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@vitest/coverage-v8": "^2.1.8",
    "esbuild": "^0.24.2",
    "eslint": "^9.17.0",
    "prettier": "3.4.2",
    "tsup": "8.3.5",
    "tsx": "^4.19.2",
    "typescript": "5.7.2",
    "vitest": "2.1.8",
    "zx": "^8.2.4"
  }
}
