{
  "$schema": "http://json.schemastore.org/tsconfig",
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "noEmit": true
  },
  "include": [
    "**/*.ts",
    "./*.cjs",
    "./*.js",
    "./*.mjs",
    "./*.mts",
    "./*.ts",
    "./*.config.cjs",
    "./*.config.js",
    "./*.config.mjs",
    "./*.config.mts",
    "./*.config.ts",
    "./.eslintrc.cjs",
    "./.eslintrc.js"
  ]
}
