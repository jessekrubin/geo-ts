{
  "extends": "@jsse/tsconfig/tsconfig.json",
  "compilerOptions": {
    "module": "Node16",
    "moduleResolution": "Node16",
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noEmit": false,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "plugins": []
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist", "./src/types-tests/**/*"]
}
