{
  "name": "@jsse/geo",
  "type": "module",
  "version": "0.0.37",
  "description": "geo typescript",
  "author": "jessekrubin",
  "license": "MIT",
  "keywords": [],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./json": {
      "types": "./dist/json.d.ts",
      "import": "./dist/json.js",
      "require": "./dist/json.cjs"
    },
    "./lib/*": {
      "types": "./dist/esm/*",
      "import": "./dist/esm/*",
      "require": "./dist/cjs/*"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "!**/*.test-data.*",
    "!**/*.test.*",
    "!**/*.tsbuildinfo",
    "CHANGELOG.md",
    "README.md",
    "dist"
  ],
  "scripts": {
    "build-fast": "tsdown src/index.ts --format cjs,esm",
    "build": "pnpm run build:tsdown && pnpm run build:tsc",
    "build:tsdown": "tsdown --format cjs,esm --dts",
    "build:tsc": "tsc -b tsconfig.build.json && tsx scripts/post-build.mts",
    "test": "vitest run",
    "cov": "vitest run --coverage",
    "dev": "vitest watch --coverage",
    "lint": "eslint .",
    "fmt": "prettier --write . --ignore-path ../../.prettierignore",
    "nodetest": "node --import tsx --test ./src/*test.ts",
    "prepublishOnly": "pnpm run build",
    "typecheck": "tsc --noEmit -p tsconfig.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@jsse/geojson": "workspace:*",
    "@jsse/geotypes": "workspace:*",
    "@turf/meta": "^7.3.1"
  },
  "devDependencies": {
    "@jsse/eslint-config": "^0.4.25",
    "@jsse/geotypes": "workspace:*",
    "@jsse/tsconfig": "^0.4.0",
    "@turf/helpers": "^7.3.1",
    "@turf/random": "^7.3.1",
    "@turf/turf": "^7.3.1",
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "benchmark": "^2.1.4",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "publint": "^0.3.16",
    "tsdown": "^0.18.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
