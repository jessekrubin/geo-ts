{
  "name": "@jsse/geo",
  "type": "module",
  "version": "0.0.37",
  "description": "geo typescript",
  "author": "jessekrubin",
  "license": "MIT",
  "keywords": [],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./json": {
      "types": "./dist/json.d.ts",
      "import": "./dist/json.js",
      "require": "./dist/json.cjs"
    },
    "./lib/*": {
      "types": "./dist/esm/*",
      "import": "./dist/esm/*",
      "require": "./dist/cjs/*"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "!**/*.test-data.*",
    "!**/*.test.*",
    "!**/*.tsbuildinfo",
    "CHANGELOG.md",
    "README.md",
    "dist"
  ],
  "scripts": {
    "build-fast": "tsdown src/index.ts --format cjs,esm",
    "build": "pnpm run build:tsdown && pnpm run build:tsc",
    "build:tsdown": "tsdown --format cjs,esm --dts",
    "build:tsc": "tsc -b tsconfig.build.json && tsx scripts/post-build.mts",
    "test": "vitest run",
    "cov": "vitest run --coverage",
    "dev": "vitest watch --coverage",
    "lint": "eslint .",
    "fmt": "prettier --write . --ignore-path ../../.prettierignore",
    "nodetest": "node --import tsx --test ./src/*test.ts",
    "prepublishOnly": "pnpm run build",
    "typecheck": "tsc --noEmit -p tsconfig.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@jsse/geojson": "workspace:*",
    "@jsse/geotypes": "workspace:*",
    "@turf/meta": "^7.2.0"
  },
  "devDependencies": {
    "@jsse/eslint-config": "^0.4.0",
    "@jsse/geotypes": "workspace:*",
    "@jsse/tsconfig": "^0.3.0",
    "@turf/helpers": "^7.2.0",
    "@turf/random": "^7.2.0",
    "@turf/turf": "^7.2.0",
    "@types/node": "^22.15.29",
    "@vitest/coverage-v8": "^3.1.4",
    "benchmark": "^2.1.4",
    "eslint": "^9.28.0",
    "prettier": "^3.5.3",
    "publint": "^0.3.12",
    "tsdown": "^0.12.6",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  }
}
