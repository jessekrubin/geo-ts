{
  "name": "@jsse/geo-ts",
  "type": "module",
  "version": "0.0.3",
  "private": true,
  "packageManager": "pnpm@9.11.0",
  "description": "geo-json-schemas by way of typebox",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "scripts": {
    "buildog": "pnpm run -r build",
    "change": "changeset",
    "dev": "vitest watch",
    "fmt": "prettier --write . --cache",
    "fmtc": "prettier --check .",
    "fmtl": "prettier --list-different .",
    "lint": "eslint .",
    "publish": "pnpm publish -r",
    "release": "pnpm run -r build && changeset publish",
    "ci:version": "changeset version && pnpm install --no-frozen-lockfile && pnpm fmt",
    "ci:publish": "pnpm publish -r",
    "test": "vitest run"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.12",
    "@jsse/eslint-config": "^0.2.32",
    "@jsse/tsconfig": "^0.2.4",
    "dprint": "^0.49.0",
    "eslint": "^9.20.0",
    "prettier": "3.5.0",
    "tsup": "8.3.6",
    "tsx": "^4.19.2",
    "typescript": "5.7.3",
    "vitest": "3.0.5"
  }
}
