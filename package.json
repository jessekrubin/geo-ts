{
  "name": "@jsse/geo-ts",
  "type": "module",
  "version": "0.0.3",
  "private": true,
  "packageManager": "pnpm@9.11.0",
  "description": "geo-json-schemas by way of typebox",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "scripts": {
    "build": "pnpm run -r build",
    "build-fast": "tsup src/index.ts --format cjs,esm",
    "build:tsup": "tsup src/index.ts --format cjs,esm --dts",
    "change": "changeset",
    "dev": "vitest watch",
    "fmt": "prettier --write . --cache",
    "fmtc": "prettier --check .",
    "fmtl": "prettier --list-different .",
    "lint": "eslint .",
    "publish": "pnpm publish -r",
    "release": "pnpm run -r build && changeset publish",
    "ci:version": "changeset version && pnpm install --no-frozen-lockfile && pnpm fmt",
    "ci:publish": "pnpm publish -r",
    "test": "vitest run"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.8",
    "@jsse/eslint-config": "^0.2.17",
    "dprint": "^0.47.2",
    "eslint": "^9.11.1",
    "prettier": "3.3.3",
    "tsup": "8.3.0",
    "tsx": "^4.19.1",
    "typescript": "5.6.2",
    "vitest": "2.1.1"
  }
}
