{
  "name": "@jsse/geo-ts",
  "type": "module",
  "version": "0.0.3",
  "private": true,
  "description": "geo-json-schemas by way of typebox",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "scripts": {
    "buildog": "pnpm run -r build",
    "change": "changeset",
    "dev": "vitest watch",
    "fmt": "prettier --write . --cache",
    "fmtc": "prettier --check .",
    "fmtl": "prettier --list-different .",
    "lint": "eslint .",
    "publish": "pnpm publish -r",
    "release": "pnpm run -r build && changeset publish",
    "ci:version": "changeset version && pnpm install --no-frozen-lockfile && pnpm fmt",
    "ci:publish": "pnpm publish -r",
    "test": "vitest run"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@jsse/eslint-config": "^0.4.25",
    "@jsse/tsconfig": "^0.4.0",
    "dprint": "^0.50.2",
    "eslint": "^9.39.2",
    "eslint-plugin-perfectionist": "~5.0.0",
    "prettier": "3.7.4",
    "tsup": "8.5.1",
    "tsx": "^4.21.0",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  }
}
